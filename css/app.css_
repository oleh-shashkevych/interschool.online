    font-weight: 400;
    line-height: 1;
    background-color: var(--mc-white);
    padding: 0;
    position: absolute;
    pointer-events: none;
    left: var(--mc-size-075);
    top: 50%;
    transform: translateY(-50%);
    transition: all .2s ease-in-out;
}

.mc-floating__label input:focus~label,
.mc-floating__label input:not(:placeholder-shown)~label,
.mc-floating__label textarea:focus~label,
.mc-floating__label textarea:not(:placeholder-shown)~label {
