/*---*/
/*БЛОКИ*/
.mc-course-section a {
    color: var(--mc-primary-text);
}

.mc-block-grey {
    border-radius: var(--mc-border-radius);
    background-color: var(--mc-primary-accent-bg);
}

.mc-block-greyer {
    border-radius: var(--mc-border-radius);
    background-color: var(--mc-secondary-accent-bg);
}

.mc-block-shadow {
    border-radius: var(--mc-border-radius);
    -webkit-box-shadow: var(--mc-box-shadow);
    box-shadow: var(--mc-box-shadow);
    background-color: var(--mc-white);
}

.mc-block-border-orange {
    border-radius: var(--mc-border-radius);
    border: 1.5px solid var(--mc-secondary);
}

.mc-block-border-grey {
    border-radius: var(--mc-border-radius);
    border: 1.5px solid var(--mc-primary-border);
}

.mc-border-top-grey {
    border-top: 1.5px solid var(--mc-primary-border);
}

.mc-border-bottom-grey {
    border-bottom: 1.5px solid var(--mc-primary-border);
}

.mc-bg-color-green {
    background-color: var(--mc-bg-color-green);
}

.mc-bg-color-pink {
    background-color: var(--mc-bg-color-pink);
}

.mc-bg-color-aqua {
    background-color: var(--mc-bg-color-aqua);
}

.mc-bg-color-yellow {
    background-color: var(--mc-bg-color-yellow);
}

.mc-bg-color-orange {
    background-color: var(--mc-bg-color-orange);
}

.mc-bg-color-blue {
    background-color: var(--mc-bg-color-blue);
}

.mc-bg-color-turqoise {
    background-color: var(--mc-bg-color-turqoise);
}

.mc-bg-color-turqoise-rgb-3 {
    background-color: rgba(var(--mc-light-turquoise-rgb), .3);
}

.mc-bg-color-egg {
    background-color: var(--mc-bg-color-egg);
}

.mc-block-rounded {
    border-radius: var(--mc-border-radius);
}

/*---*/
/*ВЫПАДАЮЩИЕ СПИСКИ, ОКНА - общие*/

.form-control {
    border-color: var(--mc-placeholder) !important;
    padding: var(--mc-size-050) var(--mc-size-075) !important;
    color: #000 !important;
    font-weight: 500;
}

.form-control:focus {
    border-color: var(--mc-pink) !important;
}

.dropdown-toggle::after {
    border: 0 !important;
    width: 24px;
    height: 12px;
    margin: 0;
    background-image: url("../images/select-arrrow.svg");
    background-repeat: no-repeat;
    background-position: right;
    background-size: 16px 12px;
    -webkit-transition: border-color 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;
    transition: border-color 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;
    -o-transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
    transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
    transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out, -webkit-box-shadow 0.15s ease-in-out;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    display: inline-block;
}

.mc-dropdown-norelative {
    position: static !important;
}

.mc-dropdown-norelative .nav-item {
    position: static !important;
}

.dropdown-menu {
    padding: 0;
    min-width: 0;
    border: 0;
    -webkit-box-shadow: var(--mc-box-shadow);
    box-shadow: var(--mc-box-shadow-large);
    border-radius: var(--mc-size-025) !important;
}

.dropdown-menu.show {
    opacity: 1
}

.dropdown-item:hover {
    background-color: var(--mc-primary-accent-bg) !important;
}

/* Ошибка в форме */
.mc-form-error {
    position: relative;
}

.mc-form-error .form-control {
    border-color: var(--mc-red) !important;
}

.mc-form-error .mc-form-message {
    display: flex;
}

.mc-form-message {
    position: absolute;
    top: 100%;
    left: var(--mc-size-100);
    display: none;
    align-items: center;
    gap: var(--mc-size-050);
    padding: var(--mc-size-025) 0;
    font-size: var(--mc-size-075);
    color: var(--mc-red);
    line-height: 1;
}

.mc-form-message .exclamation {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    width: var(--mc-size-100);
    height: var(--mc-size-100);
    font-size: var(--mc-size-075);
    color: var(--mc-white);
    background-color: var(--mc-red);
    border-radius: 50%;
}

.mc-modal-triangle {
    position: relative;
    width: 70px;
    height: 60px;
    flex-shrink: 0;
}

.mc-modal-triangle img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    position: relative;
    z-index: 2;
}

.mc-modal-text {
    width: calc(100% - 120px);
    min-height: 60px;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.mc-modal-text>*:first-child {
    line-height: 1;
}

.mc-modal-text .mc-modal-text__wrapper {
    display: block;
    width: 100%;
    padding: var(--mc-size-025) var(--mc-size-050);
    border-radius: calc(var(--mc-border-radius) / 2);
    font-size: var(--mc-size-075);
    background-color: #ffced0;
    margin-top: var(--mc-size-050);
}

#mc-send-error-modal .modal-body {
    padding: var(--mc-size-150) var(--mc-size-200);
}

#mc-send-error-modal .modal-body>div {
    gap: var(--mc-size-100);
}

#mc-send-error-modal .modal-body a {
    margin-top: var(--mc-size-100);
    line-height: 1;
    width: 60px;
}

#mc-send-continue-modal .modal-body {
    padding: var(--mc-size-150) var(--mc-size-200);
}

#mc-send-continue-modal .modal-body>div {
