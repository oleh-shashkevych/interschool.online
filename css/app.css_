@charset "UTF-8";

:root {
    /*Основные цвета*/
    --mc-white: #ffffff;
    --mc-white-rgb: 255, 255, 255;
    --mc-black: #000000;
    --mc-black-rgb: 0, 0, 0;
    --mc-green: #008f3f;
    --mc-green-rgb: 0, 143, 63;
    --mc-orange: #FF8800;
    --mc-orange-rgb: 255, 136, 0;
    --mc-darker-orange: #e26e05;
    --mc-orange-bright: #fe5e00;
    --mc-darker-grey: #272727;
    --mc-grey: #cccccc;
    --mc-grey-rgb: 204, 204, 204;
    --mc-semi-black: #111111;
    --mc-semi-black-rgb: 17, 17, 17;
    --mc-lightiest-grey: #FCFCFC;
    --mc-light-grey: #999999;
    --mc-light-grey-rgb: 153, 153, 153;
    --mc-light-turquoise: #CDE8DD;
    --mc-light-turquoise-rgb: 205, 232, 221;
    --mc-light-turquoise-accent: #a5d6be;
    --mc-fuchsia: #D04375;
    --mc-fuchsia-hover: #b63563;
    --mc-amethyst: #8C448C;
    --mc-light-amethyst: #D8B5F4;
    --mc-duck-yellow: #FF9E10;
    --mc-director-red: #FF7171;
    --mc-racing-yellow: #FFF48A;
    --mc-red: #c90000;

    --mc-pink: #ff3000;
    --mc-pink-rgb: 255, 48, 0;
    --mc-darker-pink: #E22803;
    --mc-pink-text: #f01d00;
    --mc-blue: #1d20da;
    --mc-blue-rgb: 29, 32, 218;
    --mc-blue-standart: #1D20DA;

    /*Цвета плашек*/
    --mc-bg-color-green: #b9f3ce;
    --mc-bg-color-pink: #FDE0FF;
    --mc-bg-color-aqua: #B8F9FB;
    --mc-bg-color-yellow: #FFEA98;
    --mc-bg-color-orange: #FFE8C4;
    --mc-bg-color-blue: #CAEBFF;
    --mc-bg-color-turqoise: #B3FBF0;
    --mc-bg-color-egg: #f7f3b0;

    /*Светлые цвета выделения, хайлайта.*/
    --mc-highlight-green: #b2ead5;
    --mc-highlight-green-rgb: 178, 234, 213;
    --mc-highlight-yellow: #fae7a8;
    --mc-highlight-orange: #f9d7b2;
    --mc-highlight-orange-rgb: 249, 215, 178;
    --mc-highlight-orange-hover: #ffba76;
    --mc-highlight-blue: #bce0fc;
    --mc-highlight-blue-rgb: 188, 224, 252;
    --mc-highlight-red: #fea0a7;
    --mc-highlight-red-rgb: 254, 160, 167;
    --mc-highlight-purple: #d9d1fc;
    --mc-highlight-purple-rgb: 217, 209, 252;

    /*Цвета, назначенные конкретным типам элементов*/
    --mc-placeholder: var(--mc-grey);
    --mc-placeholder-rgb: var(--mc-grey-rgb);
    --mc-light-grey-btn: #f4f4f4;
    --mc-light-grey-btn-rgb: 244, 244, 244;
    --mc-primary: var(--mc-blue);
    --mc-primary-rgb: var(--mc-white-rgb);
    --mc-secondary: var(--mc-pink);
    --mc-secondary-rgb: var(--mc-pink-rgb);
    --mc-secondary-hover: var(--mc-pink);
    --mc-primary-text: var(--mc-semi-black);
    --mc-secondary-text: #6b6b6b;
    --mc-primary-border: var(--mc-grey);
    --mc-secondary-border: #dee2e6;
    --mc-secondary-border-rgb: 222, 226, 230;
    --mc-primary-accent-bg: #F5F5F5;
    --mc-primary-accent-bg-rgb: 245, 245, 245;
    --mc-secondary-accent-bg: #dcdcdd;
    --mc-dark-accent-bg: var(--mc-darker-grey);
    --mc-dark-border: var(--mc-darker-grey);
    --mc-thirdly: var(--mc-pink-text);

    /*Темы страниц курсов*/
    --course-theme-piano: #201f1f;
    --course-theme-piano-rgb: 32, 31, 31;

    /* Стандартные значения */
    --mc-border-radius: var(--mc-size-075);
    --mc-box-shadow: 0 0 6px rgba(0, 0, 0, .12);
    --mc-box-shadow-large: 0 0 6px rgba(0, 0, 0, .25);

    /* Стандартные размеры */
    --mc-size-100: 1rem;

    --mc-size-000: calc(var(--mc-size-100) * 0);
    --mc-size-025: calc(var(--mc-size-100) * 0.25);
    --mc-size-050: calc(var(--mc-size-100) * 0.5);
    --mc-size-075: calc(var(--mc-size-100) * 0.75);

    --mc-size-125: calc(var(--mc-size-100) * 1.25);
    --mc-size-150: calc(var(--mc-size-100) * 1.5);
    --mc-size-175: calc(var(--mc-size-100) * 1.75);

    --mc-size-200: calc(var(--mc-size-100) * 2);
    --mc-size-225: calc(var(--mc-size-100) * 2.25);
    --mc-size-250: calc(var(--mc-size-100) * 2.5);
    --mc-size-275: calc(var(--mc-size-100) * 2.75);

    --mc-size-300: calc(var(--mc-size-100) * 3);
    --mc-size-325: calc(var(--mc-size-100) * 3.25);
    --mc-size-350: calc(var(--mc-size-100) * 3.5);
    --mc-size-375: calc(var(--mc-size-100) * 3.75);

    --mc-size-400: calc(var(--mc-size-100) * 4);
    --mc-size-425: calc(var(--mc-size-100) * 4.25);
    --mc-size-450: calc(var(--mc-size-100) * 4.5);
    --mc-size-475: calc(var(--mc-size-100) * 4.75);

    --mc-size-500: calc(var(--mc-size-100) * 5);
    --mc-size-525: calc(var(--mc-size-100) * 5.25);
    --mc-size-550: calc(var(--mc-size-100) * 5.5);
    --mc-size-575: calc(var(--mc-size-100) * 5.75);

    --mc-size-600: calc(var(--mc-size-100) * 6);
    --mc-size-625: calc(var(--mc-size-100) * 6.25);
    --mc-size-650: calc(var(--mc-size-100) * 6.5);
    --mc-size-675: calc(var(--mc-size-100) * 6.75);
}

html {
    font-size: 16px;
    scroll-behavior: smooth;
}

