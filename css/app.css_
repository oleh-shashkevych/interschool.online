}

.mc-ul-li-fn2 li:first-of-type {
    margin-top: 0 !important;
}

/*---*/
/*КНОПКИ - общие*/

.mc-main-btn {
    border: 1.5px solid var(--mc-secondary);
    color: var(--mc-secondary);
    -webkit-transition: all 0.1s;
    -o-transition: all 0.1s;
    transition: all 0.1s;
}

.mc-main-btn:hover,
.mc-main-btn:focus {
    color: var(--mc-white);
    border-color: var(--mc-secondary);
    background-color: var(--mc-secondary);
}

.mc-btn-group-toogle {
    border-bottom: 1px solid var(--mc-secondary-border);
}

.mc-btn-toogle-grey:hover {
    background-color: var(--mc-primary-accent-bg);
}

.btn-check:checked+.mc-btn-toogle-grey,
.btn-check:active+.mc-btn-toogle-grey,
.mc-btn-toogle-grey:active,
.mc-btn-toogle-grey.active,
.mc-btn-toogle-grey.dropdown-toggle.show {
    background-color: var(--mc-secondary-accent-bg);
}

.mc-secondary-btn {
    display: inline-block;
    padding: var(--mc-size-025) var(--mc-size-100);
    border: 1.5px solid var(--mc-black);
    border-radius: 40px;
    color: var(--mc-black);
    text-decoration: none;
    transition: color .15s ease-in-out, border-color .15s ease-in-out;
}

.mc-secondary-btn:hover,
.mc-secondary-btn:focus {
    color: var(--mc-secondary);
    border-color: var(--mc-secondary);
}

/*---*/
/*СЕЛЕКТЫ, преобразованные в выпадающие списки бутстрапа, библиотекой dselect.js*/

.dselect-wrapper {
