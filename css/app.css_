}

.dselect-wrapper.active {
    padding: var(--mc-size-050) 0 !important;
    border-color: var(--mc-secondary) !important;
}

.dselect-wrapper .form-select {
    padding-left: var(--mc-size-075) !important;
    position: relative;
    border: 0;
    padding: 0;
}

.dselect-wrapper .form-select:focus {
    outline: 0 !important;
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
}

.dselect-wrapper .dropdown-menu {
    padding: var(--mc-size-075) 0 !important;
    width: calc(100% + 3px) !important;
    margin-left: -1.5px !important;
}

.dselect-wrapper .dropdown-item,
.dselect-wrapper .dropdown-header {
    padding-left: var(--mc-size-075);
}

.dselect-wrapper .dropdown-menu[data-popper-placement="bottom-start"] {
    margin-top: var(--mc-size-075) !important;
}

.dselect-wrapper .dropdown-menu[data-popper-placement="top-start"] {
    margin-bottom: var(--mc-size-075) !important;
}

.dselect-wrapper .form-select.show {
    border: 0 !important;
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
}

.dselect-items .dropdown-item.active,
.dselect-items .dropdown-item:active {
    background-color: var(--mc-primary-accent-bg) !important;
    color: var(--mc-primary-text);
}

/*"Голый" селект без стилей*/

.mc-select-naked {
    border: 0 !important;
}

.mc-select-naked .form-select {
    background: none !important;
    background-color: transparent;
    padding-left: var(--mc-size-100) !important;
    padding-right: var(--mc-size-100);
}

.mc-select-naked.active {
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
}

/*---*/
/*Сложный селект (Выпадающий список слева; Поле ввода справа)*/
.form-group {
    position: relative;
}

.form-group label,
.form-group-select label {
    color: #000;
    font-size: var(--mc-size-100);
    font-weight: 400;
    line-height: 1;
    background-color: var(--mc-white);
