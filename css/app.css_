    margin-bottom: var(--mc-size-400);
    text-align: center;
}

.mc-heading-sub-t {
    font-size: var(--mc-size-175);
    font-weight: 700;
    margin-bottom: var(--mc-size-300);
}

.mc-heading-n {
    font-size: var(--mc-size-200);
    font-weight: 700;
