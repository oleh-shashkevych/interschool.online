    .mc-teachers-section__swiper-wrapper .arrow--left {
        display: none;
    }
}

@media (max-width: 575.5px) {
    .mc-teachers-section__icons {
        grid-template-columns: 1fr;
    }
}

@media (max-width: 575.5px) {
    .mc-teachers-section__swiper-wrapper .img-wrapper {
        width: 100%;
        height: 315px;
        max-width: 315px;
        margin: auto;
    }
}

/* Истории успеха */
.mc-success-section__item {
    display: grid;
    grid-template-columns: 4fr 5fr;
    gap: var(--mc-size-300);
    margin-bottom: calc(var(--mc-size-600) * 1.5);
}

.mc-success-section__item:last-child {
    margin-bottom: 0;
}

.mc-success-section__img-wrapper {
    width: 100%;
    height: 100%;
    min-height: 300px;
}

.mc-success-section__img-wrapper img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    border-radius: var(--mc-border-radius);
}

.mc-success-section__item-data h2 {
    font-size: var(--mc-size-150);
}

.mc-success-section__item-text-wrapper {
    max-height: 204px;
}

@media (max-width: 991.5px) {
    .mc-success-section__item {
        gap: var(--mc-size-200);
        padding: var(--mc-size-250);
    }

    .mc-success-section__item-text-wrapper {
        max-height: 108px;
    }

    .mc-success-section__img-wrapper {
        min-height: 250px;
    }
}

@media (max-width: 575.5px) {
    .mc-success-section__item {
        grid-template-columns: 1fr;
    }

    .mc-success-section__item-data h2 {
        font-size: var(--mc-size-175);
    }

    .mc-success-section__item-btn a {
        font-size: var(--mc-size-125);
    }

    .mc-success-section__item-text-wrapper {
        max-height: 240px;
    }
}

@media (max-width: 575.5px) {
    .mc-success-section__item {
        border: none;
        border-radius: 0;
        padding: 0;
    }

    .mc-success-section__item-data h2 {
        font-size: var(--mc-size-150);
    }

    .mc-success-section__item-btn a {
        font-size: var(--mc-size-100);
    }

    .mc-success-section__item-text-wrapper {
        max-height: 200px;
    }

    .mc-success-section__img-wrapper {
        min-height: 160px;
    }
}