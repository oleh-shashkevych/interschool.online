    -moz-osx-font-smoothing: grayscale !important;
    -webkit-font-feature-settings: "liga", "liga", "aalt" 0, "aalt" 0, "salt" 0, "salt" 0, "swsh" 0, "swsh" 0, "smcp" 0, "smcp" 0, "c2sc" 0, "c2sc" 0, "case" 0, "case" 0, "ordn" 0, "ordn" 0, "locl" 0, "locl" 0, "hist" 0, "hist" 0, "ss01" 0, "ss01" 0, "ss02" 0, "ss02" 0, "ss03" 0, "ss03" 0, "ss04" 0, "ss04" 0, "ss05" 0, "ss05" 0, "ss06" 0, "ss06" 0, "ss07" 0, "ss07" 0, "ss08" 0, "ss08" 0, "ss09" 0, "ss09" 0, "ss10" 0, "ss10" 0, "ss11" 0, "ss11" 0, "sups" 0, "sups" 0, "subs" 0, "subs" 0, "sinf" 0, "sinf" 0, "frac" 0, "frac" 0, "pnum" 0, "pnum" 0, "tnum" 0, "tnum" 0, "lnum" 0, "lnum" 0, "onum" 0, "onum" 0, "cpsp" 0, "cpsp" 0, "kern", "kern";
    font-feature-settings: "liga", "liga", "aalt" 0, "aalt" 0, "salt" 0, "salt" 0, "swsh" 0, "swsh" 0, "smcp" 0, "smcp" 0, "c2sc" 0, "c2sc" 0, "case" 0, "case" 0, "ordn" 0, "ordn" 0, "locl" 0, "locl" 0, "hist" 0, "hist" 0, "ss01" 0, "ss01" 0, "ss02" 0, "ss02" 0, "ss03" 0, "ss03" 0, "ss04" 0, "ss04" 0, "ss05" 0, "ss05" 0, "ss06" 0, "ss06" 0, "ss07" 0, "ss07" 0, "ss08" 0, "ss08" 0, "ss09" 0, "ss09" 0, "ss10" 0, "ss10" 0, "ss11" 0, "ss11" 0, "sups" 0, "sups" 0, "subs" 0, "subs" 0, "sinf" 0, "sinf" 0, "frac" 0, "frac" 0, "pnum" 0, "pnum" 0, "tnum" 0, "tnum" 0, "lnum" 0, "lnum" 0, "onum" 0, "onum" 0, "cpsp" 0, "cpsp" 0, "kern", "kern";
    min-width: 320px;
    line-height: 1.75;
}

@media (min-width: 576px) {
    body {
        /*Body скрыт по умолчанию для десктопных устройств, становится видимым после загрузки шрифтов -> позиционирования элементов. См. JS*/
        visibility: hidden;
    }
}

