/*---*/
/*КНОПКА ВВЕРХ*/

#mc-top-btn {
    width: 42px;
    height: 42px;
    opacity: 0;
    z-index: 1050;
    background-color: var(--mc-secondary);
    cursor: pointer;
    bottom: 25px;
    right: 25px;
    -webkit-transition: all 0.2s;
    -o-transition: all 0.2s;
    transition: all 0.2s;
    opacity: 0;
    visibility: hidden;
}

#mc-top-btn:after {
    content: '';
    width: 18px;
    height: 18px;
    color: var(--mc-white);
    position: absolute;
    left: 50%;
    top: 50%;
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-size: cover !important;
    background: url(../images/chevron-up-solid.svg), top, no-repeat;
    -webkit-transition: all 0.2s ease-out;
    -o-transition: all 0.2s ease-out;
    transition: all 0.2s ease-out;
}

#mc-top-btn:hover:after {
    top: 35%;
}

#mc-top-btn:hover {
    opacity: 1 !important;
}

